const executeArbitrage = async () => {
  const amount = ethers.utils.parseUnits("50000", 18);
  const path = [
    CONFIG.USDT, 
    "0x0782b6d8c4551B9760e74c0545a9bCD90bdc41E5", // lisUSD
    CONFIG.USDT
  ];
  
  const minProfit = amount.mul(175).div(10000);
  
  const tx = await arbitrageContract.executeArbitrage(
    amount,
    path,
    minProfit,
    { gasLimit: 800000 }
  );
};
